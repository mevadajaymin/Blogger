   <children>
      <TextArea fx:id="txtPost" prefHeight="300.0" prefWidth="600.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="30.0" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="20.0">
        <buttons>
          <Button fx:id="btnOk" mnemonicParsing="false" text="Ok" />
            <Button fx:id="btnCancel" layoutX="135.0" layoutY="15.0" mnemonicParsing="false" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
